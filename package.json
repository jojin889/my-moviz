{
    "name": "mymoviz",
    "version": "0.1.0",
    "proxy": "http://localhost:3000",
    "private": true,
    "engines": {
      "node": "12.x"
    },
    "dependencies": {
      "@fortawesome/fontawesome-svg-core": "^1.2.10",
      "@fortawesome/free-solid-svg-icons": "^5.6.1",
      "@fortawesome/react-fontawesome": "^0.1.3",
      "bootstrap": "^4.1.3",
      "react": "^16.6.3",
      "react-dom": "^16.6.3",
      "react-scripts": "2.1.1",
      "reactstrap": "^6.5.0"
    },
    "scripts": {
      "frontend": "cd front && npm start",
      "backend": "cd back && npx nodemon app.js",
      "dev": "concurrently --kill-others-on-fail \"npm run back\" \"npm run front\"",
      "heroku-postbuild": "cd front && npm install && npm run build",
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ]
  }
  